<!-- beautify ignore:start -->
<html lang="en"
	  class="light-style layout-navbar-fixed layout-menu-fixed"
	  dir="ltr"
	  data-theme="theme-default"
	  data-assets-path="/AspAdmin/assets/"
	  data-template="vertical-menu-template">
<!-- Mirrored from demos.pixinvent.com/frest-html-admin-template/html/vertical-menu-template/index.html by HTTrack Website Copier/3.x [XR&CO'2014], Tue, 08 Aug 2023 06:56:16 GMT -->
<head>
	<meta charset="UTF-8">
	<meta name='viewport' content='width=device-width, initial-scale=1.0, user-scalable=0'>
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="description" content="@ViewBag.description">
	<meta name="author" content="@ViewBag.author">
	<meta name="keywords" content="@ViewBag.keywords">

	<!-- FAVICON -->
	<meta name="robots" content="noindex,nofollow">

	@*   <link href="/css/bootstrap-datepicker.css" rel="stylesheet" />*@
	<!--Twitter-->
	<!-- TITLE -->
	<title>@ViewBag.title</title>
	<partial name="Partials/_Link"/>
	<!-- Canonical SEO -->
	@RenderSection("Links", false)
</head>

<body class="Yekan-font">
	<!-- ?PROD Only: Google Tag Manager (noscript) (Default ThemeSelection: GTM-5DDHKGP, PixInvent: GTM-5J3LMKC) -->
	<noscript>
		<iframe src="https://www.googletagmanager.com/ns.html?id=GTM-5DDHKGP"
				height="0"
				width="0"
				style="display: none; visibility: hidden"></iframe>
	</noscript>
	<!-- End Google Tag Manager (noscript) -->
	<!-- Layout wrapper -->
	<div class="layout-wrapper layout-content-navbar">
		<div class="layout-container">
			<!-- Menu -->
			@await Component.InvokeAsync("MainMenuAdminComponents")


			<!-- / Menu -->
			<!-- Layout container -->
			<div class="layout-page">
				<!-- Navbar -->

				<nav class="layout-navbar navbar navbar-expand-xl align-items-center bg-navbar-theme"
					 id="layout-navbar">
					<div class="container-fluid">
						<div class="layout-menu-toggle navbar-nav align-items-xl-center me-3 me-xl-0 d-xl-none">
							<a class="nav-item nav-link px-0 me-xl-4"
							   href="javascript:void(0)">
								<i class="bx bx-menu bx-sm"></i>
							</a>
						</div>

						<!-- Search Small Screens -->

					</div>
				</nav>

				<!-- / Navbar -->
				<!-- Content wrapper -->
				<div class="content-wrapper">
					<!-- Content -->

					<div class="container-xxl flex-grow-1 container-p-y">
						<div class="row">
							<h4 class="py-3 breadcrumb-wrapper text-end   text-md-center text-sm-end mb-4">
								<span class="text-muted fw-light">پنل مدیریت /</span>
								@ViewBag.Title
							</h4>


							@RenderBody()
						</div>
					</div>
					<!-- / Content -->
					<!-- Footer -->
					<partial name="Partials/_Footer" />
					<!-- / Footer -->

					<div class="content-backdrop fade"></div>
				</div>
				<!-- Content wrapper -->
			</div>
			<!-- / Layout page -->
		</div>

		<!-- Overlay -->
		<div class="layout-overlay layout-menu-toggle"></div>

		<!-- Drag Target Area To SlideIn Menu On Small Screens -->
		<div class="drag-target"></div>
	</div>
	<!-- / Layout wrapper -->
	<!-- Core JS -->
	<partial name="Partials/_Script" />


	@RenderSection("scripts", false)
</body>

<!-- Mirrored from demos.pixinvent.com/frest-html-admin-template/html/vertical-menu-template/index.html by HTTrack Website Copier/3.x [XR&CO'2014], Tue, 08 Aug 2023 06:57:16 GMT -->
</html>

<!-- beautify ignore:end -->
