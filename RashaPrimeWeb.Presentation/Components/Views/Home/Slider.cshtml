@model RashaPrimeWeb.Application.Common.Models.PaginatedResult<RashaPrimeWeb.Application.CQRS.Slider.Queries.GetAllSlider.GetAllSliderDto>


<!--=====hero area start=======-->
<div class="hero11-slider-area" style="">
    
    
    @foreach (var item in Model.Items)
    {

        <div class="hero-area11"
             style="
          background-image: url(@item.PathImage.ImageForSliderUrl());
         background-position: center center;
         background-size: cover;
         background-repeat: no-repeat;
             ">
            <div class="container">
                <div class="space80"></div>
                <div class="row">
                    <div class="col-lg-6">
                        <div class="main-headding11">
	                        <span class="span">
		                        <img alt=""
		                             src="/MainTemplate/consultia/Fa/assets/img/icons/headding11-span-icon.svg" />@item.Title
	                        </span>
                            
                            @Html.Raw(item.Text)
	                    
                        </div>
                    </div>
                    <div class="col-lg-6"></div>
                </div>
            </div>
        </div>
    }

</div>
<!--=====hero area start=======-->
